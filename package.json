{
  "name": "statically-web",
  "description": "The official Statically website",
  "version": "2.0.0",
  "author": "Frans Allen <frans@statically.io>",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.22",
    "@fortawesome/free-brands-svg-icons": "^5.10.2",
    "@fortawesome/free-regular-svg-icons": "^5.10.2",
    "@fortawesome/free-solid-svg-icons": "^5.10.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "gatsby": "2.19.10",
    "gatsby-plugin-manifest": "2.2.40",
    "gatsby-plugin-offline": "3.0.33",
    "gatsby-plugin-postcss": "2.1.19",
    "gatsby-plugin-purgecss": "4.0.1",
    "gatsby-plugin-react-helmet": "3.1.21",
    "gatsby-plugin-sitemap": "^2.2.26",
    "graphql": "14.5.4",
    "prop-types": "15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-helmet": "5.2.1"
  },
  "keywords": [
    "statically",
    "cdn",
    "optimization",
    "static"
  ],
  "license": "MIT",
  "scripts": {
    "analyze:lint": "eslint --ext .jsx --ext .js .",
    "analyze:prettier": "prettier --list-different '**/*.{js,css,md,json}'",
    "analyze": "npm run analyze:lint && npm run analyze:prettier",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "fix:lint": "eslint --ext .jsx --ext .js . --fix",
    "fix:prettier": "prettier --write '**/*.{js,css,md,json}'",
    "fix": "npm run fix:lint && npm run fix:prettier",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\""
  },
  "devDependencies": {
    "@tailwindcss/custom-forms": "0.2.1",
    "autoprefixer": "9.7.4",
    "babel-eslint": "10.0.3",
    "cssnano": "4.1.10",
    "eslint": "6.8.0",
    "eslint-loader": "3.0.3",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-react": "7.18.0",
    "gatsby-plugin-eslint": "2.0.8",
    "prettier": "1.19.1",
    "tailwindcss": "1.1.4"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:staticallyio/statically-web.git"
  },
  "bugs": {
    "url": "https://github.com/staticallyio/statically-web/issues"
  },
  "publishConfig": {
    "access": "public"
  }
}
